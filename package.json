{
    "name": "vid_buster",
    "version": "0.0.0",
    "description": "HLS video downloader",
    "main": "index.js",
    "scripts": {
        "build": "tsc --noEmit && parcel build",
        "start": "node dist/server.js",
        "dev": "parcel serve dev-index.html --dist-dir dist/dev --no-autoinstall",
        "clean": "rm -rf dist .parcel-cache",
        "rebuild": "npm run clean && npm run build",
        "test": "echo \"Error: no test specified\" && exit 1"
    },
    "keywords": [
        "HLS"
    ],
    "author": "ronyhe",
    "license": "MIT",
    "devDependencies": {
        "@types/node": "^20.10.4",
        "@types/react-dom": "^18.2.24",
        "chrome-types": "^0.1.246",
        "parcel": "^2.10.3",
        "parcel-reporter-static-files-copy": "^1.5.3",
        "prettier": "^3.1.1",
        "process": "^0.11.10",
        "typescript": "^5.3.3"
    },
    "targets": {
        "popup": {
            "outputFormat": "global",
            "distDir": "dist/extension/",
            "source": "src/client/popup.tsx"
        },
        "server": {
            "outputFormat": "commonjs",
            "distDir": "dist/",
            "source": "src/server/server.ts",
            "engines": {
                "node": ">= 20"
            }
        }
    },
    "dependencies": {
        "@emotion/react": "^11.11.4",
        "@emotion/styled": "^11.11.5",
        "@fontsource/roboto": "^5.0.12",
        "@mui/icons-material": "^5.15.15",
        "@mui/material": "^5.15.15",
        "react": "^18.2.0",
        "react-dom": "^18.2.0"
    },
    "staticFiles": {
        "staticPath": "extension",
        "staticOutPath": "extension"
    }
}
