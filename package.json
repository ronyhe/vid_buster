{
    "name": "vid_buster",
    "version": "0.0.0",
    "description": "HLS video downloader",
    "main": "index.js",
    "scripts": {
        "build": "npx tsc --noEmit && npx parcel build && npm run copy-files",
        "start": "node dist/server.js",
        "start-dev": "npx parcel serve dev-index.html --dist-dir dist/dev",
        "rebuild": "npm run clean && npm run build",
        "clean": "rm -rf dist && rm -rf .parcel-cache",
        "copy-files": "cp -R extension dist",
        "test": "echo \"Error: no test specified\" && exit 1",
        "clean-windows": "if exist dist rmdir /s /q dist && if exist .parcel-cache rmdir /s /q .parcel-cache",
        "rebuild-windows": "npm run clean-windows && npm run build",
        "build-windows": "npx tsc --noEmit && npx parcel build && npm run copy-files-windows",
        "copy-files-windows": "xcopy /E /I extension dist\\extension"
    },
    "keywords": [
        "HLS"
    ],
    "author": "ronyhe",
    "license": "MIT",
    "devDependencies": {
        "@types/node": "^20.10.4",
        "@types/react-dom": "^18.2.24",
        "chrome-types": "^0.1.246",
        "parcel": "^2.10.3",
        "prettier": "^3.1.1",
        "typescript": "^5.3.3"
    },
    "targets": {
        "popup": {
            "outputFormat": "global",
            "distDir": "dist/extension/",
            "source": "src/popup.tsx"
        },
        "server": {
            "outputFormat": "commonjs",
            "distDir": "dist/",
            "source": "src/server.ts",
            "engines": {
                "node": ">= 20"
            }
        }
    },
    "dependencies": {
        "@emotion/react": "^11.11.4",
        "@emotion/styled": "^11.11.5",
        "@fontsource/roboto": "^5.0.12",
        "@mui/icons-material": "^5.15.15",
        "@mui/material": "^5.15.15",
        "react": "^18.2.0",
        "react-dom": "^18.2.0"
    }
}
